{
    "request": {
        "method": "GET",
        "urlPath": "/api/flags",
        "queryParameters": {
            "expanded": {
                "or": [
                    {
                        "matches": "[a-z]+"
                    },
                    {
                        "absent": true
                    }
                ]
            }
        }
    },
    "response": {
        "status": 200,
        "bodyFileName": "flags.json",
        "headers": {
            "Content-Type": "application/json"
        }
    }
}